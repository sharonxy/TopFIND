/* the overlayed element */
.apple_overlay {

/* must be initially hidden */
	display:none;
	
	/* place overlay on top of other elements */
	z-index:10000;
	
	/* styling */
	 border: solid 1px #dfdfdf;
	 background: #fafafa;
	 background: -moz-linear-gradient(top, #f8f8f8, #fdfdfd );  
	 background: -webkit-gradient(linear, top, bottom, from(#f8f8f8), to(#fdfdfd));
	 -moz-border-radius: 4px;
	 border-radius: 4px;
	
	width:700px;	
	min-height:350px;
    max-height:800px;
    padding: 10px;
	
	/* CSS3 styling for latest browsers */
	-moz-box-shadow:10px 10px 5px 0px #444;
	-webkit-box-shadow: 10px 10px 5px 0px #444;	

}

		

/* default close button positioned on upper right corner */
.apple_overlay .close {
	background-image:url(../images/close.png);
	position:absolute; 
        right:-15px; 
        top:-15px;
	cursor:pointer;
	height:35px;
	width:35px;
    border-bottom:none;
}


/* container for external content. uses vertical scrollbar, if needed */
div.contentWrap {
	height:441px;
	overflow-y:auto;
}

.load_overlay {
	/* place overlay on top of other elements */
	z-index:20000;
		/* overlay is hidden before loading */
		display:none;
}

