.featurepanel {
 border: solid 1px #eee;
 padding: 10px;
 overflow:auto;
 max-height: 600px;
 max-width: 1050px;
}

.featurepanel .track {

 height: 100%;
 position:relative;
 clear:both;
 margin: 0 0 1px 0;
 background: #efefef;
 background: -moz-linear-gradient(top, #efefef, #dfdfdf );  
 background: -webkit-gradient(linear, top, bottom, from(#efefef), to(#dfdfdf));

}
.featurepanel a, .featurepanel a:hover {
 border: solid 1px #777;
 background:#888;
 margin-bottom: 2px;
 height: 10px; 
  -moz-border-radius: 2px;
  border-radius: 2px;
  -moz-box-shadow: 1px 1px 3px #aaa;
  -webkit-box-shadow: 1px 1px 3px #aaa;
}
.featurepanel .protein {
 background: #888; 
 height: 3px;
 margin-bottom: 5px;
 position:relative;
}

.featurepanel .tickmark {
 width: 100px;
 height: 10px;
 position:relative;
 float:left;
 margin: 0px 0px 10px 0px;;
 border-right: 1px solid #444;
 color: #444;
 text-align:right;
}

.featurepanel .tracklable {
  margin: 0 0 5px 5px;
  text-shadow: 1px 1px 2px #aaa; 
}



.featurepanel .chain {
 background: blue;
 float:left; 
 position:absolute;
  background: -moz-linear-gradient(top, #004BCF, #0040AF );  
  background: -webkit-gradient(linear, left top, left bottom, from(#004BCF), to(#0040AF));

}

.featurepanel .domain {
 background: green;
 float:left; 
 position:absolute;
  background: -moz-linear-gradient(top, #0BCF00, #09AF00 );  
  background: -webkit-gradient(linear, left top, left bottom, from(#0BCF00), to(#09AF00));

}

.featurepanel .unknown, .featurepanel .Unknown {
border-left: solid 3px yellow;
border-right: solid 3px yellow;
}
.featurepanel .acetylation {
border-left: solid 3px #FF0000;
border-right: solid 3px #FF0000;
}
.featurepanel .formylation {
border-left: solid 3px #25FF00;
border-right: solid 3px #25FF00;
}
.featurepanel .pyrrolidone-carboxylic-acid {
border-left: solid 3px #AAAA33;
border-right: solid 3px #AAAA33;
}

.featurepanel .nterm {
 font-size: 10px;
 text-align:left;
 height: 14px;
 border-top:none;
 border-right:none;
 border-bottom: solid 1px #777;
 padding: 0 0 3px 2px;
 float:left; 
 position:absolute;
 width: 20px;
 background:none;
}


.featurepanel .cterm {
 font-size: 10px;
 text-align:right;
 height: 14px;
