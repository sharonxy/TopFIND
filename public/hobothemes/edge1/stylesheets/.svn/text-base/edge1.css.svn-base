html, body {color: #193440; background: #fff;}


.page-header { 
  margin-top: 0px; 
  padding: 0 0 0;  
  color: #eee;
  background: #292929;
  background: -moz-linear-gradient(bottom, #191919, #434343 );  
  background: -webkit-gradient(linear, left bottom, left top, from(#191919), to(#434343));

}

.page-header h1 {
  margin: 0; padding: 15px 30px 5px; 
  font-family: 'Bakersville'; font-size: 36px; letter-spacing: -1.0pt;
}

.app-name a, .app-name a:hover {
 color: #eee;
 text-shadow:black 1px 1px 2px;
 display:none;
}



#logo img{
 margin: -10px 0 0 10px;
 float:left;
 width:60px;
}
#textlogo img {
 width:90px;

}

#logo a,
#textlogo a {
 border:none;
}


#textlogo a {
 border:none;
 font-size: 2.5em;
 font-face:bold;
 font-family:fantasy;
 color:#FFBC2F;
 padding: 20px;
 text-shadow:black 1px 1px 2px; 
}

/* nav */

.page-header ul {zoom: 1; overflow: hidden;}
.page-header li {float: left; margin-left: 0; list-style: none;}

  .page-header .main-nav {    
    margin: -24px 0 -3px 160px;
  }
  
  /* seperators */
  .page-header .navigation.main-nav li {
    border-right: 1px solid #444;
    border-left: 1px solid #333;
  }

  .page-header .navigation a, 
  .page-header .navigation a:hover {
    border: none; 
    color: #eee; 
    background: none;
    display: block;
    padding: 3px 5px 5px 5px;
    font-size: 12px; 
    font-weight: normal;
    text-shadow:black 1px 1px 2px;    
  } 
 
  .page-header .navigation.main-nav a {
    border-bottom: 3px solid #191919;  
  }
  .page-header .navigation.main-nav li.current a {
    border-bottom: 3px solid #EBA82F;
  }

  .page-header .navigation.main-nav li a:hover {
    background: #193440; 
    border-bottom: 3px solid #EBA82F;
  }


/* nav search */

.page-header  form {
  position:absolute;
  margin: -10px 0 0px 20px;
 }

.top-search input{
color: grey;
padding:2px;
font-size:1.8em;
border: solid 1px #ccc;
-moz-border-radius: 3px;
border-radius: 3px;
  -moz-box-shadow: 5px 5px 5px #333;
  -webkit-box-shadow: 5px 5px 5px #333;
  box-shadow: 5px 5px 5px #333;
background: white;
float:left;
}

.top-search input:focus {
color:black;
}


.top-search button {
  color:#eee;
  text-shadow:black 1px 1px 2px;  
  padding: 5px;
  margin: 2px 0 0 20px;
  background: #ffaa50;
  background:#151515;
  border: none;
  font-size: 1.2em;
  border-radius: 5px;
  -moz-border-radius: 5px;
  -moz-box-shadow: 5px 5px 5px #333;
  -webkit-box-shadow: 5px 5px 5px #333;
  box-shadow: 5px 5px 5px #333;   
}

.top-search button:hover {
background: #ffaa50;
}

#search-results-panel {
  position: absolute; top: 35px; right: 25px; z-index: 50;
  width: 350px; height: 500px; overflow: auto; 
  padding: 0 20px 20px; border: 1px solid #ddd;
  color: black; background: #f2f2f2;
}
#search-results-panel .card.linkable a {color: black;}
#search-spinner {background:black;border:1px solid #666666;opacity:0.6;padding:2px;position:absolute;right:4px;top:6px;}

.account-nav {
display:none;
}

/*.account-nav {
  float:right;
  margin: 2px 5px 0 0;
  font-size: 11px;
}*/
.account-nav li {
  float: left; 
  margin-left: 0; padding-left: 0px; 
  color: #ddd;
  list-style: none;
}
.account-nav a:hover {border-bottom: 1px dotted #ddd;}

.account-nav .dev-user-changer,
.account-nav .logged-in-as {
display:none;
}


.section.content {}
.button {color: white; background: #5B8BA0;}
.button:hover {background-color: #193440;}
.add-to-collection {background: #E6E7DE;}
.aside { background: white; border-left: dotted 1px #E8E8E8;}

/* Column Layout */

.section-group       {display: table; width: 100%;}
.section-group-inner {display: table-row;}
.section-group-inner > * {display: table-cell; vertical-align: top;}
.aside {width: 200px;} /* works with % too */
.aside { padding: 20px; border-top: 3px solid #191919;}
.left {width: 48%; float:left;}
.right {width:48%; float:right; }
.block {background: #fff; margin-bottom: 20px;}
.spacer {border-bottom: 1px dottet red;margin-bottom:10px;}
.full {clear:both; margin-top: 10px; }
