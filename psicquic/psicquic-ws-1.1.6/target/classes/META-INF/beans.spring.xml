<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:tx="http://www.springframework.org/schema/tx"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-2.5.xsd">

    <context:component-scan base-package="org.hupo.psi.mi.psicquic.ws"/>

    <bean class="org.springframework.beans.factory.annotation.AutowiredAnnotationBeanPostProcessor"/>
    
    <bean name="psicquicConfig" class="org.hupo.psi.mi.psicquic.ws.config.PsicquicConfig">
        <property name="groupId" value="uk.ac.ebi.intact.service.psicquic"/>
        <property name="artifactId" value="psicquic-ws"/>
        <property name="version" value="1.1.6"/>
        <property name="specVersion" value="1.1"/>
        <property name="indexDirectory" value="111130-topfind.index"/>
        <property name="propertiesAsStrings" value=""/>
    </bean>
   
</beans>